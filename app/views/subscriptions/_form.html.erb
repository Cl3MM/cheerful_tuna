<%= simple_form_for(@subscription) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :owner_id %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :current %>
    <%= f.input :cost %>
    <%= f.input :paid %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
